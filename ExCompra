#include <stdio.h>

int main()
{
    float a,b;
    printf("Insira o valor da compra: ");
    scanf("%f", &a);
    if (a>20)
    {
    b=a+(a*0.45);
    printf("O valor da venda é %.2f", b);
    }
    if (a<=20)
    {
    b=a+(a*0.30);
    printf("O valor da venda é %.2f", b);
    }
    return 0;
}
